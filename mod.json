{
    "geode": "3.4.0",
    "gd": {
        "android": "2.206",
        "win": "2.206",
        "mac": "2.206"
    },
    "version": "v1.0.2",
    "id": "hiimjustin000.requested_ratings",
    "name": "Requested Ratings",
    "developer": "hiimjustin000",
    "description": "A mod that replaces unrated levels' ratings with their requested ones.",
    "repository": "https://github.com/hiimjustin000/RequestedRatings",
    "dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.12.0",
            "importance": "required"
        }
    ],
    "incompatibilities": [
        {
            "id": "hiimjustin000.na_demon",
            "version": "*",
            "importance": "superseded"
        }
    ],
    "settings": {
        "na-override": {
            "name": "NA Override",
            "description": "Changes the rating of levels with a requested rating above 10 stars to NA.",
            "type": "bool",
            "default": false
        }
    },
    "tags": [
        "enhancement",
        "interface",
        "online"
    ]
}
